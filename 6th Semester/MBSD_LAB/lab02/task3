#include <msp430.h>

int main(void)
{
    WDTCTL = WDTPW | WDTHOLD;   // Stop watchdog timer
    PM5CTL0 &= ~LOCKLPM5;       // Unlock GPIO

    P1DIR |= 0xFF;  // Set all pins of P1 as output

    while (1)
    {
        P1OUT ^= 0xFF;  // Toggle all bits of P1
        __delay_cycles(100000);
    }
}